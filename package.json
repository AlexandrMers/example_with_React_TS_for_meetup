{
  "name": "ts-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.tsx",
  "scripts": {
    "build": "NODE_ENV=production AIRBRAKE_ENV=production webpack --config config/webpack.prod.js && npm run cp-public",
    "start": "npm run wds",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "wds": "NODE_ENV=development webpack-dev-server --port 3000 --config config/webpack.dev.js --open",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": "Alexandr AWdeev",
  "license": "ISC",
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/fileTransformer.ts",
      "\\.(css|scss)$": "identity-obj-proxy",
      "@/(.*)$": "<rootDir>/src/$1",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "devDependencies": {
    "@types/fork-ts-checker-webpack-plugin": "^0.4.5",
    "@types/jest": "^26.0.23",
    "@types/node": "^16.0.0",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "@types/webpack": "^5.28.0",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.14.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.2",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "eslint": "^7.30.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "prettier": "^2.3.2",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "file-loader": "^3.0.1",
    "url-loader": "^0.5.9",
    "resolve-url-loader": "^4.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "fork-ts-checker-webpack-plugin": "^6.2.12",
    "html-webpack-plugin": "^3.2.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^1.1.0",
    "cssnano": "^3.10.0",
    "dotenv-webpack": "^1.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.0.6",
    "node-sass": "^4.5.3",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "dotenv": "^8.2.0",
    "loader-utils": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "@date-io/date-fns": "^1.3.13",
    "@hookform/resolvers": "^2.6.0",
    "@material-ui/core": "^4.11.4",
    "@material-ui/pickers": "^3.3.10",
    "react-hook-form": "^7.9.0",
    "yup": "^0.32.9"
  }
}
